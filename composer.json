{
    "name": "payneteasy/php-plugin-prestashop",
    "description": "Plugin for Prestashop for payment by PaynetEasy.",
    "version": "1.0.0-dev",
    "type": "library",
    "keywords":
    [
        "payment",
        "payneteasy",
        "prestashop"
    ],
    "homepage": "http://payneteasy.com/",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Artem Ponomarenko",
            "email": "imenem@inbox.ru",
            "role": "developer"
        }
    ],
    "minimum-stability": "dev",
    "require":
    {
        "php":                                      ">=5.3.0",
        "payneteasy/php-library-payneteasy-api":    "~1.0"
    },
    "config":
    {
        "vendor-dir": "modules/payneteasy/vendor"
    },
    "archive":
    {
        "exclude":
        [
            "!/vendor",
            "doc",
            "example",
            "test",
            "composer.*",
            "LICENSE",
            "README.md",
            "nbproject",
            ".gitignore"
        ]
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0.0-dev"
        }
    }
}
